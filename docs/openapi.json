  "info": {
    "title": "API Vekino",
    "description": "API para gestión de condominios",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "auth",
      "description": "Endpoints de autenticación"
    },
    {
      "name": "condominios",
      "description": "Gestión de condominios"
    },
    {
      "name": "unidades",
      "description": "Gestión de unidades"
    }
  ],
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Servidor de desarrollo"
    },
    {
      "url": "https://api.vekino.com",
      "description": "Servidor de producción"
    }
  ],
  "components": {
    "securitySchemes": {
      "JWT-auth": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "name": "JWT",
        "description": "Token de autenticación JWT",
        "in": "header"
      },
      "cookie": {
        "type": "apiKey",
        "in": "cookie",
        "name": "better-auth.session_token",
        "description": "Cookie de sesión de Better Auth"
      }
    },
    "schemas": {
      "RegisterSuperadminDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Correo electrónico del superadministrador",
            "example": "admin@vekino.com"
          },
          "password": {
            "type": "string",
            "description": "Contraseña del superadministrador (mínimo 8 caracteres)",
            "example": "password123",
            "minLength": 8
          },
          "name": {
            "type": "string",
            "description": "Nombre completo del superadministrador",
            "example": "Juan Pérez"
          }
        },
        "required": [
          "email",
          "password",
          "name"
        ]
      },
      "LoginSuperadminDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "Correo electrónico del superadministrador",
            "example": "admin@vekino.com"
          },
          "password": {
            "type": "string",
            "description": "Contraseña del superadministrador",
            "example": "password123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateCondominioDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Nombre del condominio",
            "example": "Condominio Las Flores",
            "minLength": 3
          },
          "nit": {
            "type": "string",
            "description": "NIT / Identificación Tributaria",
            "example": "900123456-7"
          },
          "address": {
            "type": "string",
            "description": "Dirección física del condominio",
            "example": "Calle 123 #45-67"
          },
          "city": {
            "type": "string",
            "description": "Ciudad donde se encuentra el condominio",
            "example": "Bogotá"
          },
          "country": {
            "type": "string",
            "description": "País donde se encuentra el condominio",
            "example": "Colombia"
          },
          "timezone": {
            "type": "string",
            "description": "Zona horaria del condominio",
            "enum": [
              "AMERICA_BOGOTA",
              "AMERICA_MEXICO_CITY",
              "AMERICA_LIMA",
              "AMERICA_SANTIAGO",
              "AMERICA_BUENOS_AIRES",
              "AMERICA_CARACAS",
              "AMERICA_MONTEVIDEO",
              "AMERICA_ASUNCION",
              "AMERICA_LA_PAZ",
              "AMERICA_QUITO",
              "AMERICA_GUAYAQUIL",
              "AMERICA_PANAMA",
              "AMERICA_MANAGUA",
              "AMERICA_SAN_JOSE",
              "AMERICA_TEGUCIGALPA",
              "AMERICA_GUATEMALA",
              "AMERICA_SANTO_DOMINGO",
              "AMERICA_HAVANA",
              "UTC"
            ],
            "example": "AMERICA_BOGOTA"
          },
          "frontSubdomain": {
            "type": "string",
            "description": "Subdominio usado por el frontend (ej: las-flores). Solo letras minúsculas, números y guiones",
            "example": "las-flores",
            "pattern": "^[a-z0-9-]+$"
          },
          "subdomain": {
            "type": "string",
            "description": "Subdominio único para acceso (opcional, se genera automáticamente desde frontSubdomain)",
            "example": "condominio1",
            "pattern": "^[a-z0-9-]+$"
          },
          "logo": {
            "type": "string",
            "description": "URL del logo (se genera automáticamente al subir imagen)",
            "example": "https://example.com/logo.png"
          },
          "primaryColor": {
            "type": "string",
            "description": "Color principal en formato hexadecimal",
            "example": "#3B82F6",
            "pattern": "^#[0-9A-Fa-f]{6}$"
          },
          "subscriptionPlan": {
            "type": "string",
            "description": "Plan de suscripción del condominio",
            "enum": [
              "BASICO",
              "PRO",
              "ENTERPRISE"
            ],
            "example": "BASICO"
          },
          "unitLimit": {
            "type": "number",
            "description": "Límite de unidades permitidas",
            "example": 100,
            "minimum": 1
          },
          "planExpiresAt": {
            "type": "string",
            "description": "Fecha de vencimiento del plan (ISO 8601)",
            "example": "2024-12-31T23:59:59.000Z"
          },
          "activeModules": {
            "description": "Módulos activos del condominio",
            "example": [
              "reservas",
              "documentos",
              "pqrs"
            ],
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "databaseName": {
            "type": "string",
            "description": "Nombre de la base de datos (opcional, se genera automáticamente desde el nombre)",
            "example": "condominio_las_flores"
          }
        },
        "required": [
          "name"
